  <select>\r\n            <!-- <option value=\"1\" v-for=\"drink in drinkList\">코카콜라</option> -->\r\n            <!-- <option value=\"1\" :key=\"i\" v-for=\"(drink,i) in drinkList\">{{drink.drinkName}}</option> -->\r\n            <!-- v-for이라 하면 배열 안에 있는 한 요소가 들어 간다. (drink, i)라고 할시 drink의 요소와 인덱스를 가져온다. -->\r\n            <!-- key값에 반복하는 변수(i)를 넣어준다. key값은 ID같은 역할을 한다. -->\r\n            <!-- {{drink.drinkName}} 중괄호 안, drink(요소).drink(요소)의 키로 오브젝트의 값에 접근 -->\r\n            <option value=\"drink.drinkId\" :key=\"drink.drinkId\" v-for=\"drink in drinkList\">{{drink.drinkName}}</option>\r\n            <!-- key는 요소를 만들 때 알 수 있는 고유한 값이다, key값과 value값을 drinkId로 가져옴 -->\r\n        </select>\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <td>제품코드</td>\r\n                    <td>제품명</td>\r\n                    <td>가격</td>\r\n                    <td>주문수</td>\r\n                    <td>합계</td>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr :key=\"drink.drinkId\" v-for=\"drink in drinkList\">\r\n                    <!-- 행의 key값과 행의 각 컬럼들의 값들을 정해주고, for문을 돌린다. -->\r\n                    <td>{{drink.drinkId}}</td>\r\n                    <td>{{drink.drinkName}}</td>\r\n                    <td>{{drink.price}}</td>\r\n                    <td><input type=\"number\" v-model=\"drink.qty\"></td>\r\n                    <td>{{drink.price * drink.qty}}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    name: \"\",\r\n    components: {},\r\n    data() {\r\n        return {\r\n            drinkList: [{\r\n        drinkId: \"1\",\r\n        drinkName: \"코카콜라\",\r\n        price: 700,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"2\",\r\n        drinkName: \"오렌지주스\",\r\n        price: 1200,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"3\",\r\n        drinkName: \"커피\",\r\n        price: 500,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"4\",\r\n        drinkName: \"물\",\r\n        price: 700,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"5\",\r\n        drinkName: \"보리차\",\r\n        price: 1200,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"6\",\r\n        drinkName: \"포카리\",\r\n        price: 1000,\r\n        qty: 1\r\n      },\r\n      {\r\n        drinkId: \"7\",\r\n        drinkName: \"뽀로로\",\r\n        price: 1300,\r\n        qty: 1\r\n      }\r\n    ]\r\n        };\r\n    },\r\n    setup() {},\r\n    created() {},\r\n    mounted() {},\r\n    unmounted() {},\r\n    methods: {}\r\n}\r\n</script>\r\n<style scoped>\r\ntable {\r\n    width: 100%;\r\n}\r\n\r\ntable,\r\nth,\r\ntd {\r\n    border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n    border: 1px solid black;\r\n}\r\n</style>\r\n\r\n"]}]}